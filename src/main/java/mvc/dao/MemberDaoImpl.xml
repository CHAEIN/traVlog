<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mvc.dao.MemberDao">

	<select id="memberCount" parameterType="mvc.dto.Member" resultType="int">
		SELECT COUNT(*) FROM MEMBER WHERE MEMID=#{memid} AND MEMPW=#{mempw}
	</select>
	
	<select id="gerMemberOne" parameterType="mvc.dto.Member" resultType="mvc.dto.Member">
		SELECT * FROM MEMBER WHERE MEMID=#{memid} AND MEMPW=#{mempw}
	</select>
	
</mapper>